<AuthorizeView>
    <Authorized>
        <a href="Identity/Account/Manage">Hello, @context.User.Identity?.Name!</a>
       
        <a href="logout">Log out</a>

    </Authorized>
    <NotAuthorized>
        <a href="signUp">Register</a>
        <a href="signIn">Log in</a>
    </NotAuthorized>
</AuthorizeView>
